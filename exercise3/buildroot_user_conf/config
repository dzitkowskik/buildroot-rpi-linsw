global.linux.bootargs.dyn.root="root=/dev/mmcblk0p2 rootfstype=ext3 rw rootwait"

rootfs_loc=disk
rootfs_type=ext4
rootfs_part_linux_dev=mmcblk0p2
rootfsimage=rootfs.ext4
kernel_part=disk0.1
kernelimage=uImage
bootargs="console=ttyAMA0,115200 root=/dev/mmcblk0p2 rootwait"

